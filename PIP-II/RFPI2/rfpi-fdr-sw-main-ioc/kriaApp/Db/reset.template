#### Alarm #####
record(bi, "$(acc):$(sys)_$(group)_$(num):Alarm_r") {
    field(DESC, "Alarm reading params")
    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT=1))KRIA_ALARM_R")
    field(SCAN, "I/O Intr")
}

record(stringin, "$(acc):$(sys)_$(group)_$(num):AlarmMsg_r") {
    field(DESC, "Error message")
    field(DTYP, "asynOctetRead")
    field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT=1))KRIA_ALARM_MSG_R")
    field(SCAN, "I/O Intr")
}

record(bo, "$(acc):$(sys)_$(group)_$(num):AlarmAck_s") { 
    field(DESC, "Ack alarm")
    field(DTYP, "asynInt32")
    field(OUT,  "@asyn($(PORT),$(ADDR),$(TIMEOUT=1))KRIA_ALARM_ACK_S")
    field(SCAN, "Passive")
}
