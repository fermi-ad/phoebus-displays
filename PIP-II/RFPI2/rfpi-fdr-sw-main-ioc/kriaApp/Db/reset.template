
record(dfanout, "$(acc):$(sys)_$(group)_$(num):ResetAll1_s") {
    field(DESC, "Reset all signals")
    field(DOL,  "1")
    field(OMSL, "closed_loop")
    field(OUTA, "$(acc):$(sys)_$(group)_FEP1_C1:Reset_s.PROC")
    field(OUTB, "$(acc):$(sys)_$(group)_FEP1_C2:Reset_s.PROC")
    field(OUTC, "$(acc):$(sys)_$(group)_FEP1_C3:Reset_s.PROC")
    field(OUTD, "$(acc):$(sys)_$(group)_FEP1_C4:Reset_s.PROC")
    field(OUTE, "$(acc):$(sys)_$(group)_NIRP1:Reset_s.PROC")
    field(OUTF, "$(acc):$(sys)_$(group)_NIRP2:Reset_s.PROC")
    field(OUTG, "$(acc):$(sys)_$(group)_NIRP3:Reset_s.PROC")
    field(OUTH, "$(acc):$(sys)_$(group)_NIRP4:Reset_s.PROC")
    field(FLNK, "$(acc):$(sys)_$(group)_$(num):ResetAll2_s.PROC")
}

