  <widget type="embedded" version="2.0.0">
    <name>Embedded Display_16</name>
    <macros>
      <DESC>Amp $(INSTANCE) Central $(ID) Pwr Refl:</DESC>
      <PV>$(SYS)_Central$(ID):PowerRefl_RB</PV>
    </macros>
    <file>PIP2-TS-sigmaphi75-text-readback.bob</file>
